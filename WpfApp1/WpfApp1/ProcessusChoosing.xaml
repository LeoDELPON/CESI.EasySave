<Window x:Class="WpfApp1.ProcessusChoosing"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="ProcessusChoosing" Height="300" Width="200" Padding="0">
    <StackPanel>
        <ListBox Height="199" Width="192" Name="ListCB" >
      
               
    
        </ListBox>
        <StackPanel Orientation="Horizontal">
            <Button Content="{DynamicResource Minus}" Width="98" Click="MinusBtn_Click" />
            <Button Content="{DynamicResource Plus}" Width="100" Click="PlusBtn_Click"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
     
            <Button Content="{DynamicResource OK}" Width="100" Name="OkBtn"/>
        </StackPanel>



    </StackPanel>


</Window>
